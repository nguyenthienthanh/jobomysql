{
  "name": "jobomysql",
  "version": "1.0.0",
  "description": "Fetch mysql from another",
  "author": "Nguyen Thien Thanh - Jobo",
  "main": "app.js",
  "scripts": {
    "build": "babel -w code/ -d build -s",
    "start": "better-npm-run start-prod-api",
    "dev": "better-npm-run start-dev-api",
    "lint": "jshint --exclude node_modules .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "start-prod-api": {
      "command": "nodemon ./bin/api --exec babel-node",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "SSLPORT": 5134,
        "APIPORT": 5126
      }
    },
    "start-dev-api": {
      "command": "nodemon ./bin/api --exec babel-node",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "SSLPORT": 5134,
        "APIPORT": 3080
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fbsamples/messenger-platform-samples.git"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-plugin-typecheck": "^3.6.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.8",
    "body-parser": "^1.15.0",
    "config": "^1.20.4",
    "cors": "^2.8.4",
    "express": "^4.13.4",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.4",
    "mysql": "^2.14.1",
    "pretty-error": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "0.1.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-import": "^0.8.0"
  },
  "engines": {
    "node": "~4.1.2"
  }
}
